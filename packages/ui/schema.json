{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "components": {
      "description": "Array of component names",
      "items": {
        "type": "string"
      },
      "type": "array"
    },
    "prefix": {
      "description": "Prefix string for the components",
      "type": "string"
    }
  },
  "required": ["components", "prefix"]
}
