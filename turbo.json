{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"]
    },
    "clean": {
      "cache": false
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "format": {
      "dependsOn": ["^build"]
    },
    "format:check": {
      "dependsOn": ["^build"]
    },
    "lint": {},
    "lint:fix": {},
    "test": {},
    "test:coverage": {},
    "typecheck": {
      "dependsOn": ["^build"]
    }
  }
}
